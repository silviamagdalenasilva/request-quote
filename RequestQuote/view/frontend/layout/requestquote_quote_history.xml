<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Cochez\RequestQuote\Block\Quote\History" name="requestquote.quote.history" cacheable="false">
                <container name="requestquote.quote.history.info" as="info" label="Quote History Info"/>
                <container name="requestquote.quote.history.extra.column.header" as="extra.column.header" label="Quote History Extra Column Header"/>
            </block>
            <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="Magento_Customer::account/link/back.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
